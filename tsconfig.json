{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@screens":[ "src/screens"],
      "@components":[ "src/components"],
      "@utils":[ "src/utils"],
      "@config/*":[ "src/config/*"],
      "@contexts/*":[ "src/contexts/*"],
      "@assets/*":[ "assets/*"]
    }
  }
}
